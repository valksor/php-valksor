{
    "name": "valksor/php-functions-number",
    "description": "A PHP library providing various number-related functions including prime number checking, distance calculations, and mathematical operations",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "number",
        "math",
        "prime",
        "distance",
        "calculation",
        "gcd",
        "lcm",
        "valksor",
        "symfony"
    ],
    "homepage": "https://github.com/valksor/php-valksor",
    "require": {
        "php": ">=8.4",
        "valksor/php-functions-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "suggest": {
        "ext-gmp": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Functions\\Number\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valksor\\Functions\\Number\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
