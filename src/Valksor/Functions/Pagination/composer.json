{
    "name": "valksor/php-functions-pagination",
    "description": "A PHP library providing pagination functionality to generate page numbers for UI pagination controls, with smart handling of large page sets",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "pagination",
        "page numbers",
        "ui",
        "navigation",
        "valksor",
        "symfony"
    ],
    "homepage": "https://github.com/valksor/valksor",
    "require": {
        "php": ">=8.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Functions\\Pagination\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
