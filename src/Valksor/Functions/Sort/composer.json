{
    "name": "valksor/php-functions-sort",
    "description": "A PHP library providing various sorting algorithms and utilities for arrays and objects",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "sort",
        "sorting",
        "algorithm",
        "bubble sort",
        "merge sort",
        "array",
        "object",
        "valksor",
        "symfony"
    ],
    "homepage": "https://github.com/valksor/php-valksor",
    "require": {
        "php": ">=8.4",
        "valksor/php-functions-iteration": "^1.0",
        "valksor/php-functions-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Functions\\Sort\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valksor\\Functions\\Sort\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
