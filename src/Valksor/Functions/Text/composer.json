{
    "name": "valksor/php-functions-text",
    "description": "A PHP library providing enhanced text manipulation functions for string operations, formatting, and transformations",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "text",
        "string",
        "manipulation",
        "formatting",
        "transformation",
        "valksor",
        "symfony"
    ],
    "homepage": "https://github.com/valksor/php-valksor",
    "require": {
        "php": ">=8.4",
        "symfony/intl": "*",
        "symfony/string": "*",
        "valksor/php-functions-iteration": "^1.0",
        "valksor/php-functions-local": "^1.0",
        "valksor/php-functions-php": "^1.0",
        "valksor/php-functions-preg": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Functions\\Text\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valksor\\Functions\\Text\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
