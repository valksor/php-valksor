{
    "name": "valksor/php-functions-memoize",
    "description": "A PHP library providing memoization functionality to cache the results of expensive function calls and return the cached result when the same inputs occur again",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "memoize",
        "memoization",
        "cache",
        "performance",
        "optimization",
        "valksor",
        "symfony"
    ],
    "homepage": "https://github.com/valksor/php-valksor",
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "suggest": {
        "valksor/php-bundle": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Functions\\Memoize\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valksor\\Functions\\Memoize\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
