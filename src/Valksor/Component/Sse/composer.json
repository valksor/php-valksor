        "php": ">=8.4",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "psr/link": "*",
        "symfony/asset": "*",
        "symfony/asset-mapper": "*",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-client-contracts": "*",
        "symfony/http-foundation": "*",
        "symfony/web-link": "*",
        "twig/twig": "*",
        "valksor/php-bundle": "^1.0",
