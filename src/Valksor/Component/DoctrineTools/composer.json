{
    "name": "valksor/php-doctrine-tools",
    "description": "Database utilities and enhanced tools for Doctrine ORM including migrations, console commands, and performance optimizations",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "doctrine",
        "database",
        "orm",
        "migrations",
        "tools",
        "utilities",
        "valksor",
        "dbal"
    ],
    "homepage": "https://github.com/valksor/php-valksor",
    "require": {
        "php": ">=8.4",
        "doctrine/dbal": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/migrations": "*",
        "doctrine/orm": "*",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/dependency-injection": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/event-dispatcher": "*",
        "symfony/finder": "*",
        "symfony/serializer": "*",
        "symfony/uid": "*",
        "valksor/php-bundle": "^1.0",
        "valksor/php-functions-preg": "^1.0",
        "valksor/php-functions-text": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "conflict": {
        "doctrine/dbal": "<4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Valksor\\Component\\DoctrineTools\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
